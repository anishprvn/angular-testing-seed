{
  "name": "angular2-webpack-seed",
  "version": "0.1.0",
  "description": "My Angular2-TypeScript App",
  "keywords": [
    "angular2",
    "typescript",
    "webpack"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "clean:dist": "rimraf dist",
    "clean:compiled": "rimraf compiled",
    "build:dev": "webpack --progress --colors --profile --bail --config config/webpack/webpack.dev.js",
    "build:qa": "webpack --progress --colors --profile --bail --config config/webpack/webpack.qa.js",
    "build:prod:aot": "webpack --progress --colors --profile --bail --config config/webpack/webpack.prod.js",
    "build:watch": "webpack --watch --progress --colors --profile --bail",
    "start": "node app.js",
    "dev": "npm run clean:dist && npm run clean:compiled && npm run build:dev",
    "qa": "npm run clean:dist && npm run clean:compiled && npm run build:qa",
    "prod": "npm run clean:dist && npm run clean:compiled && npm run build:prod:aot"
  },
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@types/core-js": "^0.9.37",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.8",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.11",
    "angular-router-loader": "^0.6.0",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "body-parser": "^1.17.1",
    "codelyzer": "^3.0.0-beta.3",
    "css-loader": "^0.27.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.7.0",
    "karma": "^1.7.0",
    "ngc-webpack": "^1.2.0",
    "node-config-manager": "^1.2.0",
    "node-sass": "^4.5.0",
    "optimize-js-plugin": "^0.0.4",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "source-map": "^0.5.6",
    "style-loader": "^0.13.2",
    "to-string-loader": "^1.1.5",
    "tslib": "^1.6.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.2",
    "webpack": "^2.2.1",
    "webpack-merge": "^4.0.0"
  },
  "license": "MIT"
}
